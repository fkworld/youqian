{
  "private": true,
  "scripts": {
    "start": "npx umi dev",
    "build": "npx umi build",
    "postinstall": "npx umi generate tmp",
    "prettier": "npx prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "npx umi-test",
    "test:coverage": "npx umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.15",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.28",
    "antd": "^4.8.4",
    "lint-staged": "^10.0.7",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "umi": "^3.2.28",
    "yorkie": "^2.0.0"
  }
}
